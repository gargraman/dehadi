{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "DATABASE_URL=postgresql://dehadi:dehadi_password@localhost:5436/dehadi_db SESSION_SECRET=dev-session-secret-at-least-32-chars NODE_ENV=development PORT=5000 tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "docker:build": "docker-compose build",
    "docker:build:dev": "docker-compose build --target development app-dev",
    "docker:build:prod": "docker-compose build --target production app-prod",
    "docker:dev": "docker-compose --profile dev up",
    "docker:dev:build": "docker-compose --profile dev up --build",
    "docker:dev:tools": "docker-compose --profile dev --profile tools up",
    "docker:prod": "docker-compose --profile prod up",
    "docker:prod:build": "docker-compose --profile prod up --build",
    "docker:test": "docker-compose --profile test up --abort-on-container-exit",
    "docker:down": "docker-compose down",
    "docker:down:volumes": "docker-compose down -v",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:app": "docker-compose logs -f app-dev app-prod",
    "docker:logs:db": "docker-compose logs -f postgres",
    "docker:ps": "docker-compose ps",
    "docker:restart": "docker-compose restart",
    "docker:db:seed:test": "docker-compose exec app-dev tsx db/seed-test.ts",
    "docker:db:seed:dev": "docker-compose exec app-dev tsx db/seed-dev.ts",
    "docker:db:push": "docker-compose exec app-dev npm run db:push",
    "docker:shell": "docker-compose exec app-dev sh",
    "docker:db:shell": "docker-compose exec postgres psql -U dehadi -d dehadi_db",
    "docker:clean": "docker-compose down -v --remove-orphans && docker system prune -f"
  },
